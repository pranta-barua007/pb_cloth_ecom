{
    "name": "pb_clothing_server",
    "version": "1.0.0",
    "proxy": "http://localhost:8000",
    "engines": {
        "node": "14.16.0",
        "npm": "6.14.11"
    },
    "scripts": {
        "client": "cd client && npm start",
        "server": "nodemon server.js",
        "build": "cd client && npm run build",
        "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
        "start": "node server.js",
        "heroku-postbuild": "cd client && npm install --only=prod && npm install --only=prod --no-shrinkwrap && npm run build"
    },
    "dependencies": {
        "compression": "1.7.4",
        "cors": "2.8.5",
        "dotenv": "8.2.0",
        "express": "^4.16.4",
        "express-sslify": "^1.2.0",
        "helmet": "^4.6.0",
        "morgan": "^1.10.0",
        "stripe": "^8.191.0"
    },
    "devDependencies": {
        "concurrently": "^6.4.0",
        "nodemon": "^2.0.15"
    }
}
